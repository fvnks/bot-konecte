{
  "name": "botito-whatsapp",
  "version": "1.0.0",
  "description": "Bot de WhatsApp para categorizar informaci√≥n de grupos con IA de Gemini",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon --config nodemon.json server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "tunnel": "node scripts/tunnel/start-tunnel.js",
    "dev:tunnel": "node scripts/tunnel/start-tunnel.js & nodemon --config nodemon.json server.js",
    "noip": "node scripts/noip/update-noip-webhook.js",
    "dev:noip": "node scripts/noip/update-noip-webhook.js & nodemon --config nodemon.json server.js",
    "setup:noip": "bash scripts/noip/setup-noip.sh",
    "setup:nginx": "sudo bash scripts/noip/setup-nginx-proxy.sh",
    "update:nginx": "sudo node scripts/noip/update-nginx-proxy.js",
    "setup:hosts": "sudo bash scripts/noip/setup-hosts.sh",
    "check": "node scripts/utils/check-status.js",
    "start:all": "node scripts/utils/start-all.js",
    "ngrok-noip:install": "cd scripts/ngrok-noip && npm install",
    "ngrok-noip:setup": "cd scripts/ngrok-noip && npm run setup",
    "ngrok-noip:start": "cd scripts/ngrok-noip && npm start",
    "ngrok-noip:service": "sudo ./scripts/ngrok-noip/install-service.sh",
    "cloudflare:tunnel": "node scripts/cloudflare/start-cloudflare-tunnel.js",
    "cloudflare:webhook": "node scripts/cloudflare/update-cloudflare-webhook.js",
    "cloudflare:start": "node scripts/cloudflare/start-all-cloudflare.js",
    "cloudflare:setup": "sudo bash scripts/cloudflare/setup-cloudflare.sh",
    "cloudflare:pm2": "pm2 start botito-cloudflare-tunnel botito-cloudflare-webhook-updater",
    "whatsapp:fix": "node fix-whatsapp.js",
    "whatsapp:clean": "node fix-whatsapp.js clean",
    "whatsapp:deps": "sudo bash install-whatsapp-deps.sh"
  },
  "keywords": [
    "whatsapp",
    "bot",
    "gemini",
    "nodejs",
    "express",
    "mysql",
    "baileys"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@whiskeysockets/baileys": "^6.7.2",
    "axios": "^1.9.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.19.2",
    "form-data": "^4.0.2",
    "google-auth-library": "^9.11.0",
    "googleapis": "^140.0.0",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.10.0",
    "node-cron": "^4.1.0",
    "openai": "^5.0.1",
    "puppeteer": "^24.10.0",
    "qrcode-terminal": "^0.12.0",
    "string-similarity": "^4.0.4",
    "whatsapp-web.js": "^1.30.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.3"
  }
}
